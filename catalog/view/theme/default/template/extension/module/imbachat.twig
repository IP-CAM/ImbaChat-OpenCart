{% if true %}
    <script src="https://dev.imbachat.com/imbachat/v1/{{ dev_id }}/widget"></script>
    <script>
        function imbachatWidget(){
            if(!window.ImbaChat){
                return setTimeout(imbachatWidget, 50);
            }
            window.ImbaChat.load({{params|raw}})
        }
        imbachatWidget();
    </script>
{% endif %}